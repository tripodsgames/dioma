{
  "name": "dioma",
  "version": "0.4.3",
  "description": "Elegant dependency injection container for vanilla JavaScript and TypeScript",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:zheksoon/dioma.git"
  },
  "keywords": [
    "di",
    "dependency injection",
    "di container",
    "inversion of control",
    "ioc",
    "ioc container",
    "javascript",
    "typescript",
    "dioma",
    "inversify",
    "awilix",
    "tsyringe"
  ],
  "type": "module",
  "main": "dist/dioma.cjs",
  "module": "dist/dioma.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/dioma.js",
      "require": "./dist/dioma.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "vite build --minify=false --sourcemap=true",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.4.0",
    "vite": "^5.2.7",
    "vitest": "^1.4.0",
    "vite-plugin-dts": "^3.8.1"
  }
}
